    "roi": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        }
      },
      "root": "projects/roi",
      "sourceRoot": "projects/roi/src",
      "prefix": "roi",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
