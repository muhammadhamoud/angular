            "index": "projects/roi/src/index.html",
            "main": "projects/roi/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/roi/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              "projects/roi/src/favicon.ico",
              "projects/roi/src/assets"
            ],
            // "styles": [
            //   "projects/roi/src/styles.scss"
            // ],
            "styles": [
              {
                "input": "projects/roi/src/styles.scss"
              },
              {
                "input": "projects/roi/src/highlightjs/material-light.scss"
              },
              {
                "inject": false,
                "input": "projects/roi/src/styles/custom-themes/pink-bluegrey.scss",
                "bundleName": "pink-bluegrey"
              },
              {
                "inject": false,
                "input": "projects/roi/src/styles/custom-themes/deeppurple-amber.scss",
                "bundleName": "deeppurple-amber"
              },
              {
                "inject": false,
                "input": "projects/roi/src/styles/custom-themes/indigo-pink.scss",
                "bundleName": "indigo-pink"
              },
              {
                "inject": false,
                "input": "projects/roi/src/styles/custom-themes/purple-green.scss",
                "bundleName": "purple-green"
              }
            ],
            "scripts": []
